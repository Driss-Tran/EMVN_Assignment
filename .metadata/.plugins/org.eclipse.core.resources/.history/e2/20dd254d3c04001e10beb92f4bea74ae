package data_access_object;

import java.util.ArrayList;

import entities.Track;

public interface DAOInterface<T> {
	// Defining some methods of Application
	
	public int addTrack(T value);
	
	public Track viewDetails(T index,T value); 
	
	public int updateTrack(T value);
	
	public int deleteTrack(T value);
	
	public ArrayList<T> viewAll();
}
